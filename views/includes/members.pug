div
  .container.mx-auto.pb-10
    +title('Members')
    .members(class='grid overflow-hidden py-16 px-10 grid-cols-1 grid-rows-3 md:grid-cols-2 md:grid-rows-2 lg:grid-cols-3 lg:grid-rows-1 gap-10')
      each val in ['hazem', 'ahmed', 'tamer', 'karim', 'hosam', 'reda']
        .box.relative.pt-16
          .flex.items-center
            .flex-1
              img.w-full.rounded-lg(src='./public/pics/tiger.jpg' alt='photo')
            h4(class="flex flex-col items-center justify-around h-40 text-base m-auto p-4")
              a(href='https://facebook.com')
                p f
              a(href='https://twitter.com')
                p t
              a(href='https://eg.linkedin.com')
                p l
              a(href='https://youtube.com')
                p y
          .info
            h3.text-2xl.py-3(class='pl-[23%]')
              span.text-blue-500 Name: 
              span.text-gray-500.capitalize= val
            p.text-black.text-xl.py-3.pb-4(class='pl-[23%]') Simple Short Description
